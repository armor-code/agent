[supervisord]
nodaemon=true

[program:AGENT_1]
command=python3 /usr/src/app/worker.py --index 1
user=root
autostart=true
autorestart=true
restart_delay=5
stdout_logfile=/temp/agent_1.log
stderr_logfile=/temp/agent_1-error.log

